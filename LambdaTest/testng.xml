  <parameter name="browserName" value="Chrome"></parameter>
  <parameter name="browserVersion" value="128.0"></parameter>
  <parameter name="osVersion" value="Windows 10"></parameter>
    <classes>
      <class name="LTDemo.LTParallel"/>
