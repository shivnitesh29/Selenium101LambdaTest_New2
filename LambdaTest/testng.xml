  </test> 
  
  <test name="EdgeTest">
  <parameter name="browserName" value="Edge"></parameter>
  <parameter name="browserVersion" value="127.0"></parameter>
  <parameter name="osVersion" value="macOS Ventura"></parameter>
    <classes>
      <class name="LTDemo.LTParallel"/>
    </classes>
  </test> 
</suite> 
